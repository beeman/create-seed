{
  "bin": {
    "create-seed": "dist/cli.mjs"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "commander": "^14.0.3",
    "giget": "^3.1.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@changesets/cli": "^2.29.8",
    "@intellectronica/ruler": "^0.3.34",
    "@types/bun": "latest",
    "pkg-pr-new": "^0.0.62",
    "publint": "^0.3.16",
    "tsdown": "^0.20.3"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "name": "create-seed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beeman/create-seed.git"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "scripts": {
    "build": "tsdown",
    "changeset": "changeset",
    "check-types": "tsc -b --noEmit",
    "lint": "biome check --error-on-warnings",
    "lint:fix": "biome check --error-on-warnings --write",
    "release": "changeset publish",
    "ruler": "ruler apply",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "version": "changeset version && bun lint:fix"
  },
  "type": "module",
  "types": "./dist/index.d.mts",
  "version": "1.1.1"
}
